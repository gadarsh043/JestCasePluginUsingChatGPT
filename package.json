{
  "name": "write-jest-code",
  "displayName": "Write Jest Code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "write-jest-code.listFunctions",
        "title": "List All Methods"
      },
      {
        "command": "write-jest-code.explainFunctions",
        "title": "Need input on this Methods"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "write-jest-code.listFunctions",
          "when": "editorTextFocus",
          "group": "quinbay-assist@1"
        },
        {
          "command": "write-jest-code.explainFunctions",
          "when": "editorHasSelection",
          "group": "quinbay-assist@2"
        }
      ],
      "commandPalette": [
        {
          "command": "write-jest-code.listFunctions",
          "title": "List All Methods"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "write-jest-code-sidebar",
          "title": "Quinbay Assist",
          "icon": "./icons/iteration.png"
        }
      ]
    },
    "views": {
      "write-jest-code-sidebar": [
        {
          "id": "write-jest-code-sidebar",
          "name": "Quinbay Assist",
          "webviewPanel": "write-jest-code-sidebar",
          "contextualTitle": "Hello"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "markdown-it": "^13.0.1",
    "openai": "^3.2.1"
  }
}
